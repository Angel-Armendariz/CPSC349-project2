                    object
                    key:text="@innerText"
                    edit:text>
                    {{default todo.text "New todo item"}}
                     </li>
                    {{/for}}

                </ul>
                <button new:todo class="container mx-auto text-white text-center bg-green-500 shadow-lg shadow-indigo-500 rounded-md p-3 m-4">Add Todo</button>

