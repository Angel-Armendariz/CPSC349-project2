                    <span>&nbsp;</span>
                    <span object key:sunday="@innerText" edit:sunday:without-remove>{{default sunday "Enter Date"}}</span>
                    <span>,&nbsp;</span>
                    <span>{{years}}</span>

                </h3>


                 <ul  array key="firstPageSundayTodo" sortable>

                    {{#for todo in firstPageSundayTodo}}
